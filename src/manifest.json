{
  "manifest_version": 2,
  "name": "WebExtensions Commands UI",
  "version": "1.0",
  "icons": { "16": "img/icons/16.png", "48": "img/icons/48.png", "128": "img/icons/128.png" },
  "browser_action": { "default_title": "WebExtensions Commands UI", "default_icon": { "16": "img/icons/16.png", "24": "img/icons/24.png", "32": "img/icons/32.png" } },
  "options_ui": { "page": "options.html", "browser_style": true, "chrome_style": true },
  "background": { "scripts": ["background.js"] },
  "commands": { "paper": { "description": "Paper" }, "material": { "description": "Material" }, "photon": { "description": "Photon" }},
  "permissions": ["storage"]
}