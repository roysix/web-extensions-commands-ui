{
  "manifest_version": 2,
  "name": "WebExtensions Commands UI",
  "version": "1.0",
  "icons": { "16": "icon.png", "48": "icon.png", "128": "icon.png" },
  "browser_action": { "default_title": "WebExtensions Commands UI", "default_icon": { "16": "icon.png", "24": "icon.png", "32": "icon.png" } },
  "options_ui": { "page": "options.html" },
  "background": { "scripts": ["background.js"] },
  "commands": { "photon": { "description": "Photon" }, "material": { "description": "Material" }, "paper": { "description": "Paper" }},
  "permissions": ["storage"]
}